pipeline {
    agent { label 'jdk8' }
    triggers { pollSCM('0 * * * *') }
    stages {
        stage('SourceCode') {
            steps {
                git branch: 'main', url: 'https://github.com/sainath335/js-e2e-express-server.git'
            }
        }
        stage('Build') {
            steps {
                sh 'npm install'
                sh 'npm run build'
                sh 'npm test'
            }
        }
    
    }
}